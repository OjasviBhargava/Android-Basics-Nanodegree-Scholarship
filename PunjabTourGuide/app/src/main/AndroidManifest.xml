<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ojasvi.punjabtourguide">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".PatialaActivity" android:label="Patiala" android:parentActivityName=".MainActivity"/>
        <activity android:name=".JalandharActivity" android:label="Jalandhar" android:parentActivityName=".MainActivity"/>
        <activity android:name=".ChandigarhActivity" android:label="Chandigarh" android:parentActivityName=".MainActivity"/>
        <activity android:name=".AmritsarActivity" android:label="Amritsar" android:parentActivityName=".MainActivity"/>
    </application>
    <uses-permission android:name="android.permission.INTERNET" />
</manifest>